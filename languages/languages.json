{
  "python:3.8": {
    "prefixes": [
      "py",
      "python",
      "gyp"
    ],
    "file": "main.py",
    "command": "python main.py"
  },
  "c": {
    "prefixes": [
      "c",
      "h"
    ],
    "file": "main.c",
    "command": "gcc main.c -o main && ./main"
  },
  "haskell": {
    "prefixes": [
      "haskell",
      "hs"
    ],
    "file": "Main.hs",
    "command": "ghc Main.hs && ./Main"
  },
  "java": {
    "prefixes": [
      "java",
      "jsp"
    ],
    "file": "Main.java",
    "command": "javac Main.java && java Main"
  },
  "cpp": {
    "prefixes": [
      "cpp",
      "hpp",
      "cc",
      "hh",
      "c++",
      "h++",
      "cxx",
      "hxx"
    ],
    "file": "main.cpp",
    "command": "g++ main.cpp -o main && ./main"
  }
}
